{
	"manifest_version": 2,
	"name": "Link Opening Extension",
	"version": "1.0",
	"content_scripts": [
	  {
	    "matches": [
	      "https://*/*"
	    ],
	    "js": ["jquery-3.0.0.min.js","content.js"]
	  }
	],
	"browser_action": {
		"default_icon": "favicon.png"
	},
	"background": {
		"scripts": ["second.js"]
	}

}