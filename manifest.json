{
	"manifest_version": 2,
	"name": "Link Opening Extension",
	"description": "This extension will open the first 10 links in a webpage and record the rest",
	"version": "1.0",
	"content_scripts": [
	  {
	    "matches": [
	      "https://*/*"
	    ],
	    "js": ["jquery-3.0.0.min.js","content.js"]
	  }
	],
	"browser_action": {
		"default_icon": "favicon.png",
		"default_popup": "index.html"
	},
	"background": {
		"scripts": ["second.js"]
	},
	"permissions": [
		"activeTab"
	]
}